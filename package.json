{
	"name": "gods-hera",
	"displayName": "gods-hera",
	"description": "Extension to exchange files with Zeus",
	"version": "0.0.18",
	"publisher": "daanrutten",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Extension Packs",
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/daanrutten/gods-hera.git"
	},
	"activationEvents": [
		"onCommand:extension.updateSource",
		"onCommand:extension.updateAssets",
		"onCommand:extension.cleanAssets",
		"onCommand:extension.updateRoles",
		"onCommand:extension.createProject",
		"onCommand:extension.compileHtml"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.updateSource",
				"title": "Update source",
				"category": "Hera"
			},
			{
				"command": "extension.updateAssets",
				"title": "Update assets",
				"category": "Hera"
			},
			{
				"command": "extension.cleanAssets",
				"title": "Clean assets",
				"category": "Hera"
			},
			{
				"command": "extension.updateRoles",
				"title": "Update roles",
				"category": "Hera"
			},
			{
				"command": "extension.createProject",
				"title": "Create project",
				"category": "Hera"
			},
			{
				"command": "extension.compileHtml",
				"title": "Compile HTML",
				"category": "Hera"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "extension.updateSource",
					"group": "Hera"
				},
				{
					"command": "extension.updateAssets",
					"group": "Hera"
				},
				{
					"command": "extension.cleanAssets",
					"group": "Hera"
				},
				{
					"command": "extension.updateRoles",
					"group": "Hera"
				},
				{
					"command": "extension.createProject",
					"group": "Hera"
				}
			],
			"editor/context": [
				{
					"when": "resourceLangId == html && editorHasSelection",
					"command": "extension.compileHtml",
					"group": "Hera"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production"
	},
	"dependencies": {
		"axios": "^0.21.1",
		"firebase": "^8.2.1",
		"form-data": "^3.0.0",
		"less": "^4.0.0",
		"parse5": "^6.0.1",
		"strip-json-comments": "^3.1.1",
		"typescript": "^4.1.3"
	},
	"devDependencies": {
		"@types/less": "^3.0.2",
		"@types/node": "^14.14.17",
		"@types/parse5": "^5.0.3",
		"@types/vscode": "^1.40.0",
		"@typescript-eslint/eslint-plugin": "^4.11.1",
		"@typescript-eslint/parser": "^4.11.1",
		"eslint": "^7.16.0",
		"ts-loader": "^8.0.4",
		"webpack": "^5.11.1",
		"webpack-cli": "^4.3.1"
	}
}
